{
  "embed_dim": 10,
  "rnn_units": 64,
  "num_layers": 2,
  "cheb_order": 2,
  "bidir_adj_mx": true,
  "scaler": "standard",
  "load_external": true,
  "batch_size": 32,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": false,
  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.003,
  "lr_decay": true,
  "lr_scheduler": "multisteplr",
  "lr_decay_ratio": 0.75,
  "steps": [
    5,
    15,
    30,
    40
  ],
  "clip_grad_norm": false,
  "use_early_stop": true,
  "patience": 50
}
